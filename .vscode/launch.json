{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DocumentIntelligence API (.NET 8)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DocumentIntelligence.API/bin/Debug/net8.0/DocumentIntelligence.API.dll",
            "cwd": "${workspaceFolder}/DocumentIntelligence.API",
            "console": "integratedTerminal"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ],
    "inputs": [
        {
            "type": "promptString",
            "id": "programPath",
            "description": "Path to the built DocumentIntelligence.API.dll relative to the workspace folder (for example: DocumentIntelligence.API/bin/Debug/net8.0/DocumentIntelligence.API.dll)"
        }
    ]
}
